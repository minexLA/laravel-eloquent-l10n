{
    "name": "msafadi/laravel-eloquent-l10n",
    "description": "Laravel Eloquent Localization",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "translations",
        "localization",
        "l10n"
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/container": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/config": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.5|^11.0",
        "mockery/mockery": "2.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Safadi\\Eloquent\\L10n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Safadi\\Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Mohammed Safadi",
            "email": "m.safadi@hotmail.com"
        }
    ],
    "minimum-stability": "dev"
}
